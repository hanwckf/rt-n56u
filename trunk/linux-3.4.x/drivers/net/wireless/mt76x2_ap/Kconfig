config MT76X2_AP
	tristate "MediaTek MT7602E/7612E 802.11n/ac AP Support"
	select WIRELESS_EXT
	select WEXT_SPY
	select WEXT_PRIV

config  MT76X2_AP_LED
	bool "LED Support"
	depends on MT76X2_AP
	default y

config  MT76X2_AP_WSC
	bool "WSC (WiFi Simple Config)"
	depends on MT76X2_AP

