	bool "Auto Channel Selection Enhancement"
	default y
	depends on RT5392_AP

config  RT5392_AP_80211N_DRAFT3
	bool "802.11n Draft3"
	depends on RT5392_AP

config  RT5392_AP_MCAST_RATE_SPECIFIC
	bool "User specific TX rate of mcast packets"
