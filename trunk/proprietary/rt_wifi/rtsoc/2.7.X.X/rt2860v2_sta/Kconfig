config  RT2860V2_STA_SNMP
	bool "Net-SNMP Support"
	depends on RT2860V2_STA

config  RT2860V2_STA_P2P
	bool "Wireless Direct(P2P)"
	depends on RT2860V2_STA

config  RT2860V2_STA_READ_MAC_FROM_MTD
	bool "Read and set MAC from MTD/EEPROM"
	default y
	depends on RT2860V2_STA

config  RT2860V2_STA_ATE
	bool "Provide ATE+QA testmode commands support"
	default n
	depends on RT2860V2_STA

config  RT2860V2_STA_DBG
	bool "Debug support for AP mode"
	default n
	depends on RT2860V2_STA

